#include <stdio.h>

char *ft_strcpy(char *dest, const char *src)
{
    size_t i = 0;
    
    while (src[i] != '\0')
    {
        dest[i] = src[i];
        i++;
    }
    dest[i] = '\0';
    return(dest);
}

int main()
{
    const char *p = "Banana";
    char apple[6];
    printf("%s", ft_strcpy(apple, p));
    return 0;
}
